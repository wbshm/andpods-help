<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">

    <% if (theme.favicon_url) { %>
        <link rel="icon" href="<%- url_for(theme.favicon_url) %>"/>
    <% } %>

    <title>
        <% if (page.title) { %>
            <%= page.title + ' - ' + config.title %>
        <% } else{ %>
            <%= config.title %>
        <% } %>
    </title>

    <!-- Spectre.css framework -->
    <%- css('spectre/dist/spectre.min') %>
    <%- css('spectre/dist/spectre-exp.min') %>
    <%- css('spectre/dist/spectre-icons.min') %>

    <!-- theme css & js -->
    <%- css('css/book') %>
    <%- js('js/book') %>

    <!-- tocbot -->
    <%- js('js/tocbot.4.4.2.min') %>
    <%- css('css/tocbot.4.4.2.min') %>

    <!-- katex -->
    <%- css('css/KaTeX.min') %>

    <%- partial('_lib/google-analytics') %>
    <%- partial('_lib/zooming-image') %>
</head>
